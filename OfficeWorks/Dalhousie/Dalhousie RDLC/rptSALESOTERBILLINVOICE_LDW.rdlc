      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT A.SALESBILLNO, A.SALESBILLDATE, A.ITEMDESC, A.SERIALNO, A.QUANTITY, A.UOMCODE, A.RATE, A.AMOUNT, A.GROSSAMT, 
       A.AMTINWORD, A.EX1, A.EX2, A.EX3, A.EX4, A.EX5, 
       A.PARTYNAME, A.PARTYADDRESS, A.PARTYCITY, A.PARTYSTATE, A.PARTYPIN, A.PARTYVATNO, A.PARTYPANNO, 
       A.COMPANYNAME, A.COMPANYADDRESS, A.COMPANYADDRESS1, A.COMPANYADDRESS2, A.COMPANYADDRESS3, A.COMPANYCITY, A.COMPANYSTATE, 
       A.COMPANYCOUNTRY, A.COMPANYPHONE, A.COMPANYFAX, A.COMPANYEMAIL, A.WEBSITE, A.WBSTNO, A.CSTNO, A.VATNO, A.CINNO, 
       A.PONO, A.PODATE, A.MRNO, A.MRDATE, B.COPY, NULL SCRAPSALESBILLNO, NULL SCRAPSALESBILLDATE
FROM
(SELECT M.SALESBILLNO, M.SALESBILLDATE, D.ITEMDESC, D.SERIALNO, D.QUANTITY, D.UOMCODE, D.RATE, D.AMOUNT, M.GROSSAMT, 
       fn_num_to_words(M.GROSSAMT,'RS') AMTINWORD, M.GATEPASSNO EX1, TO_CHAR(M.GATEPASSDATE,'DD/MM/YYYY') EX2, NULL EX3, NULL EX4, NULL EX5, 
       P.PARTYNAME, P.PARTYADDRESS, P.PARTYCITY, P.PARTYSTATE, P.PARTYPIN, P.VATNO PARTYVATNO, P.PANNO PARTYPANNO, 
       C.COMPANYNAME, C.COMPANYADDRESS, C.COMPANYADDRESS1, C.COMPANYADDRESS2, C.COMPANYADDRESS3, C.COMPANYCITY, C.COMPANYSTATE, 
       C.COMPANYCOUNTRY, C.COMPANYPHONE, C.COMPANYFAX, C.COMPANYEMAIL, C.WEBSITE, C.WBSTNO, C.CSTNO, C.VATNO, C.CINNO, 
       M.PURCHASEORDERNO PONO, M.PURCHASEORDERDATE PODATE, M.MRNO, M.MRDATE  
  FROM SALESOTHERBILLMASTER M, SALESOTHERBILLDETAIL D, PARTYMASTER P, COMPANYMAST C 
 WHERE M.COMPANYCODE = 'CJ0048' 
   AND M.DIVISIONCODE = '0001' 
   AND M.COMPANYCODE = C.COMPANYCODE 
   AND M.COMPANYCODE=D.COMPANYCODE 
   AND M.DIVISIONCODE=D.DIVISIONCODE 
   AND M.SALESBILLNO=D.SALESBILLNO 
   AND M.SALESBILLDATE=D.SALESBILLDATE 
   AND M.COMPANYCODE=P.COMPANYCODE 
   AND M.PARTYCODE=P.PARTYCODE 
   AND P.MODULE='SALES' 
UNION ALL
SELECT DISTINCT M.SALESBILLNO, M.SALESBILLDATE, CM.CHARGESHORTNAME ITEMDESC, TO_NUMBER(CM.SERIALNO) SERIALNO, 0 QUANTITY, NULL UOMCODE, 0 RATE, CD.CHARGEAMOUNT AMOUNT, M.GROSSAMT, 
       fn_num_to_words(M.GROSSAMT,'RS') AMTINWORD, M.GATEPASSNO EX1, TO_CHAR(M.GATEPASSDATE,'DD/MM/YYYY') EX2, NULL EX3, NULL EX4, NULL EX5, 
       P.PARTYNAME, P.PARTYADDRESS, P.PARTYCITY, P.PARTYSTATE, P.PARTYPIN, P.VATNO PARTYVATNO, P.PANNO PARTYPANNO, 
       C.COMPANYNAME, C.COMPANYADDRESS, C.COMPANYADDRESS1, C.COMPANYADDRESS2, C.COMPANYADDRESS3, C.COMPANYCITY, C.COMPANYSTATE, 
       C.COMPANYCOUNTRY, C.COMPANYPHONE, C.COMPANYFAX, C.COMPANYEMAIL, C.WEBSITE, C.WBSTNO, C.CSTNO, C.VATNO, C.CINNO, 
       M.PURCHASEORDERNO PONO, M.PURCHASEORDERDATE PODATE, M.MRNO, M.MRDATE  
  FROM SALESOTHERBILLMASTER M, SALESOTHERBILLDETAIL D, PARTYMASTER P, CHARGEMASTER CM, SALESCHARGEDETAILS CD, COMPANYMAST C 
 WHERE M.COMPANYCODE = 'CJ0048' 
   AND M.DIVISIONCODE = '0001' 
   AND M.COMPANYCODE = C.COMPANYCODE 
   AND M.COMPANYCODE=D.COMPANYCODE 
   AND M.DIVISIONCODE=D.DIVISIONCODE 
   AND M.SALESBILLNO=D.SALESBILLNO 
   AND M.SALESBILLDATE=D.SALESBILLDATE 
   AND M.COMPANYCODE=P.COMPANYCODE 
   AND M.PARTYCODE=P.PARTYCODE 
   AND P.MODULE='SALES' 
   AND M.COMPANYCODE=CD.COMPANYCODE 
   AND M.DIVISIONCODE=CD.DIVISIONCODE 
   AND M.SALESBILLNO=CD.DOCUMENTNO 
   AND M.SALESBILLDATE=CD.DOCUMENTDATE 
   AND M.CHANNELCODE=CD.CHANNELCODE 
   AND M.COMPANYCODE=CM.COMPANYCODE 
   AND M.DIVISIONCODE=CM.DIVISIONCODE 
   AND M.CHANNELCODE=CM.CHANNELCODE 
   AND CD.CHARGECODE=CM.CHARGECODE 
   AND P.MODULE=CM.MODULE 
 ORDER BY SALESBILLNO, SERIALNO
) A,
(
 SELECT 1 SRL, 'ORIGINAL' COPY FROM DUAL
 UNION ALL
 SELECT 2 SRL, 'DUPLICATE' COPY FROM DUAL
 UNION ALL
 SELECT 3 SRL, 'TRIPLICATE' COPY FROM DUAL
) B
ORDER BY A.SALESBILLNO, B.SRL, A.SERIALNO</CommandText>
